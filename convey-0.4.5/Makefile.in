# Automatic linking to create UPC copies of C files.
%__upc.upc: %.c
	@echo "INFO: Linking UPC copy of   C file: $@"
	@rm -f $@
	@test -d $(dir $@) || $(MKDIR_P) $(dir $@)
	@if test "$(top_srcdir)" = "$(top_builddir)"; \
  then $(LN_S) $(notdir $<) $@ || cp -f $< $@; \
  else $(LN_S) $< $@ || cp -f $< $@; fi

